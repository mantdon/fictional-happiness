{% extends 'Employee/base.html.twig' %}

{% block panels %}
    {% include 'Employee/employee_panels.html.twig' with {
        home_active: 'true'
    }%}
{% endblock %}

{% block panel_content %}
    <div class="profile">
        <div class="row h-100">
            <div class="col-sm-12 profile-panels-area" style="margin-top: 0; border-top: none">
                <div>
                    <ul class="list-group">
                        <li class="list-group-item first-item">{{ user.emailname }}</li>
                        <li class="list-group-item"><strong>Vardas: </strong> {{ user.firstname }} {{ user.lastname }}</li>
                        <li class="list-group-item"><strong>Email: </strong>{{ user.email }}</li>
                        <li class="list-group-item"><strong>Miestas: </strong>{{  user.city }}</li>
                        <li class="list-group-item"><strong>Adresas: </strong>{{ user.address }}</li>
                        <li class="list-group-item"><strong>Telefono nr: </strong>{{ user.phone }}</li>
                        <li class="list-group-item last-item"><strong>UÅ¾siregistruota: </strong>{{ user.registrationDate|date('Y/m/d') }}</li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
{% endblock %}